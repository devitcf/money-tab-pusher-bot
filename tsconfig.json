{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "types": ["node"],
    "moduleResolution": "node",
    //    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"]
}
